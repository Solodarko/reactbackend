# ===========================================
# BACKEND ENVIRONMENT CONFIGURATION TEMPLATE
# ===========================================
# Copy this file to .env and fill in your values

# Server Configuration
PORT=5000
NODE_ENV=development

# Database Configuration
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority&appName=AppName

# Authentication
JWT_SECRET=your_jwt_secret_here

# Frontend Configuration
FRONTEND_URL=http://localhost:5173
# FRONTEND_URL=https://your-production-frontend-url.com

# Zoom API Configuration
ZOOM_ACCOUNT_ID=your_zoom_account_id_here
ZOOM_CLIENT_ID=your_zoom_client_id_here
ZOOM_CLIENT_SECRET=your_zoom_client_secret_here

# Zoom Webhook Configuration
ZOOM_WEBHOOK_SECRET_TOKEN=your_webhook_secret_token_here
